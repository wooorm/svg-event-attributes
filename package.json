    "hast-util-select": "^4.0.0",
    "hast-util-to-string": "^1.0.0",
    "prettier": "^2.0.0",
    "rehype-parse": "^7.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "tape": "^5.0.0",
    "unified": "^9.0.0",
    "xo": "^0.38.0"
  },
  "scripts": {
    "generate": "node build",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
